                        ["controller", {"controllerName":"twoStepLaunch", "isEnabled":true, "launchRPM":2500}]
                        ["exhaust", {"name": "mainEngine", "mufflingOffset": -0.2, "gainOffset": 1.5}],
                        ["transmission", {"defaultForwardMode": "S"}]

                        //Misc
                        ["controller", {"controllerName":"adaptiveBrakeLights", "isEnabled":false}]

                        //UI
                        ["simpleControlButton", {"buttonId":"dse", "uiName":"DSE","icon":"powertrain_esc","color":"8ED500", "offColor":"343434", "offColorElectric":"dseWarningPulse"}]
                    ],
                },
                "off": {
                    "name": "ESC & TC Off",
                    "order": 50,
                    "settings": [
                        ["type"]
                        //ESC & TCS
                        ["controller", {"controllerName":"tractionControl", "isEnabled":false}]
                        ["controller", {"controllerName":"yawControl", "isEnabled":false}]
                        ["controller", {"controllerName":"motorTorqueControl"}],
                        ["controller", {"controllerName":"brakeControl"}],
                        ["controller", {"controllerName":"awdControl"}],
                        ["controller", {"controllerName":"diffControl"}],

                        //AWD
                        ["controller", {"controllerName":"electronicSplitShaftLock", "isEnabled":true, "minimumLock":0, "avDiffThreshold": 27}]

                        //Diffs
                        ["controller", {"controllerName":"lockRear", "isEnabled":true, "avDiffThreshold":4}]

                        //Suspension
                        ["controller", {"controllerName":"adaptiveFrontDamper", "damperMode":"hard"}]
                        ["controller", {"controllerName":"adaptiveRearDamper", "damperMode":"hard"}]

                        ["controller", {"controllerName":"adaptiveFrontSwayBar", "torsionBarMode":"hard"}]
                        ["controller", {"controllerName":"adaptiveRearSwayBar", "torsionBarMode":"hard"}]

                        //Engine & Transmission
                        ["controller", {"controllerName":"twoStepLaunch", "isEnabled":false}]
                        ["exhaust", {"name": "mainEngine", "mufflingOffset": -0.2, "gainOffset": 1.5}],
                        ["transmission", {"defaultForwardMode": "S"}],

                        //Misc
                        ["controller", {"controllerName":"adaptiveBrakeLights", "isEnabled":false}]

